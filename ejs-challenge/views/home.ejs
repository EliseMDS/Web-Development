<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= homeStart %></p>
  
<div>
    <% homePosts.forEach(element => { %>
    <h1><%= element.title %></h1>
    <p><%= _.truncate(element.content, {"length": 150, "separator": ' ',"omission": '...'}) %> <a href="/posts/<%= element._id %>">Read More</a></p>
   
    <% }); %>
</div>

<%- include("partials/footer"); -%>